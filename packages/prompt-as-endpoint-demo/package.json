{
  "name": "prompt-as-endpoint-demo",
  "version": "1.0.0",
  "description": "Demo application showcasing the prompt-as-endpoint package using Hono web framework",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "start": "node dist/index.js",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "dev:verbose": "tsx watch --inspect src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "demo",
    "hono",
    "typescript",
    "web-server",
    "prompt-as-endpoint"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.9.8",
    "@hono/node-server": "^1.19.3",
    "prompt-as-endpoint": "file:../prompt-as-endpoint"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
